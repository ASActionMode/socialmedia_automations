{
  "manifest_version": 3,
  "name": "Facebook Automation",
  "version": "0.6.1",
  "description": "Automate Facebook friend requests with customizable delays.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
